        },
        {
          "idNum": 1,
          "orderNum": 1,
          "pcNode": false,
          "linkTo": 0,
          "ShowOnlyOnce": true,
          "NodeIsActive": true,
          "NodePortraitBitmap": "",
          "NodeNpcName": "",
          "NodeSound": "none",
          "conversationText": "You enter a large room with a strange floor made of glassy tiles. In the centre of the room is a thin, bald, unhealthy looking man with grey, saggging skin.\n\nSomething is very wrong. You stop in the doorway. ",
          "IsExpanded": true,
          "subNodes": [
            {
              "idNum": 40,
              "orderNum": 0,
              "pcNode": true,
              "linkTo": 0,
              "ShowOnlyOnce": false,
              "NodeIsActive": true,
              "NodePortraitBitmap": "",
              "NodeNpcName": "",
              "NodeSound": "none",
              "conversationText": "Continue",
              "IsExpanded": true,
              "subNodes": [
                {
                  "idNum": 41,
                  "orderNum": 0,
                  "pcNode": false,
                  "linkTo": 0,
                  "ShowOnlyOnce": false,
                  "NodeIsActive": true,
                  "NodePortraitBitmap": "",
                  "NodeNpcName": "",
                  "NodeSound": "none",
                  "conversationText": "The man sees you and looks up weakly. Flakes of dust and grit are falling from his face, and the hand that he waves at you leaves a glistening trail of fine debris in its wake. His legs seem to have gone completely - no doubt the large pile of grey dust he is now lingering in.",
                  "IsExpanded": true,
                  "subNodes": [
                    {
                      "idNum": 42,
                      "orderNum": 0,
                      "pcNode": true,
                      "linkTo": 0,
                      "ShowOnlyOnce": false,
                      "NodeIsActive": true,
                      "NodePortraitBitmap": "",
                      "NodeNpcName": "",
                      "NodeSound": "none",
                      "conversationText": "Continue",
                      "IsExpanded": true,
                      "subNodes": [
                        {
                          "idNum": 43,
                          "orderNum": 0,
                          "pcNode": false,
                          "linkTo": 0,
                          "ShowOnlyOnce": false,
                          "NodeIsActive": true,
                          "NodePortraitBitmap": "",
                          "NodeNpcName": "",
                          "NodeSound": "none",
                          "conversationText": "\"You should not have come.\" His voice is raspy, hoarse. Flakes of dry dust spit from his mouth as he speaks. \"This place is not safe.\"",
                          "IsExpanded": true,
                          "subNodes": [
                            {
                              "idNum": 44,
                              "orderNum": 0,
                              "pcNode": true,
                              "linkTo": 0,
                              "ShowOnlyOnce": false,
                              "NodeIsActive": true,
                              "NodePortraitBitmap": "",
                              "NodeNpcName": "",
                              "NodeSound": "none",
                              "conversationText": "\"What's happening? Is it contagious?\"",
                              "IsExpanded": true,
                              "subNodes": [
                                {
                                  "idNum": 45,
                                  "orderNum": 0,
                                  "pcNode": false,
                                  "linkTo": 0,
                                  "ShowOnlyOnce": false,
                                  "NodeIsActive": true,
                                  "NodePortraitBitmap": "",
                                  "NodeNpcName": "",
                                  "NodeSound": "none",
                                  "conversationText": "\"I don't know. Maybe.\" The man waves at you again, and you see that his fingers have been reduced to flaking stumps, a bit like a thousand year old statue left exposed to the fates of the weather. \"You've disturbed the air by coming here from the outside. You've sped up the decline. I won't last long now.\"",
                                  "IsExpanded": true,
                                  "subNodes": [
                                    {
                                      "idNum": 46,
                                      "orderNum": 0,
                                      "pcNode": true,
                                      "linkTo": 0,
                                      "ShowOnlyOnce": false,
                                      "NodeIsActive": true,
                                      "NodePortraitBitmap": "",
                                      "NodeNpcName": "",
                                      "NodeSound": "none",
                                      "conversationText": "\"Are you Vinazl? I come to you for help.\"",
                                      "IsExpanded": true,
                                      "subNodes": [
                                        {
                                          "idNum": 47,
                                          "orderNum": 0,
                                          "pcNode": false,
                                          "linkTo": 0,
                                          "ShowOnlyOnce": false,
                                          "NodeIsActive": true,
                                          "NodePortraitBitmap": "",
                                          "NodeNpcName": "",
                                          "NodeSound": "none",
                                          "conversationText": "\"Yes, I am Vinazl.\" The man laughs bitterly. \"But it seems I am more in need of help than you are.\"",
                                          "IsExpanded": true,
                                          "subNodes": [
                                            {
                                              "idNum": 48,
                                              "orderNum": 0,
                                              "pcNode": true,
                                              "linkTo": 0,
                                              "ShowOnlyOnce": false,
                                              "NodeIsActive": true,
                                              "NodePortraitBitmap": "",
                                              "NodeNpcName": "",
                                              "NodeSound": "none",
                                              "conversationText": "\"What can I do to help you?\"",
                                              "IsExpanded": true,
                                              "subNodes": [
                                                {
                                                  "idNum": 49,
                                                  "orderNum": 0,
                                                  "pcNode": false,
                                                  "linkTo": 0,
                                                  "ShowOnlyOnce": false,
                                                  "NodeIsActive": true,
                                                  "NodePortraitBitmap": "",
                                                  "NodeNpcName": "",
                                                  "NodeSound": "none",
                                                  "conversationText": "\"Nothing. Its too late. I'm going more and more quickly. Soon I won't be able to speak. I can feel my throat falling into my stomach. My stomach spilling onto the floor like so much dry dust.\"\n\n\n\n\"You must be quick. I may be only able to answer a couple of questions. What is it that you want?\"",
                                                  "IsExpanded": true,
                                                  "subNodes": [
                                                    {
                                                      "idNum": 50,
                                                      "orderNum": 0,
                                                      "pcNode": true,
                                                      "linkTo": 0,
                                                      "ShowOnlyOnce": false,
                                                      "NodeIsActive": true,
                                                      "NodePortraitBitmap": "",
                                                      "NodeNpcName": "",
                                                      "NodeSound": "none",
                                                      "conversationText": "\"What happened to you?\"",
                                                      "IsExpanded": true,
                                                      "subNodes": [
                                                        {
                                                          "idNum": 54,
                                                          "orderNum": 0,
                                                          "pcNode": false,
                                                          "linkTo": 0,
                                                          "ShowOnlyOnce": false,
                                                          "NodeIsActive": true,
                                                          "NodePortraitBitmap": "",
                                                          "NodeNpcName": "",
                                                          "NodeSound": "none",
                                                          "conversationText": "\"You waste your question on that?\" Vinzal laughs. \"What a crazy world we live in. The true answer is - I do not know. About two days ago, I began... drying out. My skin grew grey and hard. And then it began flaking. Slowly at first, and then more quickly, and now an avalanche.\" A chunk falls from his head and shatters into dust on the floor. ",
                                                          "IsExpanded": true,
                                                          "subNodes": [
                                                            {
                                                              "idNum": 55,
                                                              "orderNum": 0,
                                                              "pcNode": true,
                                                              "linkTo": 0,
                                                              "ShowOnlyOnce": false,
                                                              "NodeIsActive": true,
                                                              "NodePortraitBitmap": "",
                                                              "NodeNpcName": "",
                                                              "NodeSound": "none",
                                                              "conversationText": "Continue.",
                                                              "IsExpanded": true,
                                                              "subNodes": [
                                                                {
                                                                  "idNum": 56,
                                                                  "orderNum": 0,
                                                                  "pcNode": false,
                                                                  "linkTo": 0,
                                                                  "ShowOnlyOnce": false,
                                                                  "NodeIsActive": true,
                                                                  "NodePortraitBitmap": "",
                                                                  "NodeNpcName": "",
                                                                  "NodeSound": "none",
                                                                  "conversationText": "\"It must be a curse. But from who? The squirrels are abominations, but they can't speak or cast spells. The undead, then...? Perhaps. I don't know. I haven't seen anything to suggest they can cast spells. They don't seem to have the intelligence for it.\"",
                                                                  "IsExpanded": true,
                                                                  "subNodes": [
                                                                    {
                                                                      "idNum": 57,
                                                                      "orderNum": 0,
                                                                      "pcNode": true,
                                                                      "linkTo": 0,
                                                                      "ShowOnlyOnce": false,
                                                                      "NodeIsActive": true,
                                                                      "NodePortraitBitmap": "",
                                                                      "NodeNpcName": "",
                                                                      "NodeSound": "none",
                                                                      "conversationText": "Continue",
                                                                      "IsExpanded": true,
                                                                      "subNodes": [
                                                                        {
                                                                          "idNum": 58,
                                                                          "orderNum": 0,
                                                                          "pcNode": false,
                                                                          "linkTo": 0,
                                                                          "ShowOnlyOnce": false,
                                                                          "NodeIsActive": true,
                                                                          "NodePortraitBitmap": "",
                                                                          "NodeNpcName": "",
                                                                          "NodeSound": "none",
                                                                          "conversationText": "\"Which suggests that there is something... something else in the forest. Something powerful, capable of great magics.\" \n\nVinazl pauses. His right arm is nearly completely gone. His head is lopsided. He is fading quickly.",
                                                                          "IsExpanded": true,
                                                                          "subNodes": [
                                                                            {
                                                                              "idNum": 59,
                                                                              "orderNum": 0,
                                                                              "pcNode": true,
                                                                              "linkTo": 0,
                                                                              "ShowOnlyOnce": false,
                                                                              "NodeIsActive": true,
                                                                              "NodePortraitBitmap": "",
                                                                              "NodeNpcName": "",
                                                                              "NodeSound": "none",
                                                                              "conversationText": "\"Did you have anything to do with the squirrels or the undead?\"",
                                                                              "IsExpanded": true,
                                                                              "subNodes": [
                                                                                {
                                                                                  "idNum": 72,
                                                                                  "orderNum": 0,
                                                                                  "pcNode": false,
                                                                                  "linkTo": 0,
                                                                                  "ShowOnlyOnce": false,
                                                                                  "NodeIsActive": true,
                                                                                  "NodePortraitBitmap": "",
                                                                                  "NodeNpcName": "",
                                                                                  "NodeSound": "none",
                                                                                  "conversationText": "\"No. I have done nothing except protect the forest.\" His voice is getting smaller, more hollow. \"I don't know why any creature would target me.\"",
                                                                                  "IsExpanded": true,
                                                                                  "subNodes": [
                                                                                    {
                                                                                      "idNum": 73,
                                                                                      "orderNum": 0,
                                                                                      "pcNode": true,
                                                                                      "linkTo": 0,
                                                                                      "ShowOnlyOnce": false,
                                                                                      "NodeIsActive": true,
                                                                                      "NodePortraitBitmap": "",
                                                                                      "NodeNpcName": "",
                                                                                      "NodeSound": "none",
                                                                                      "conversationText": "Continue",
                                                                                      "IsExpanded": true,
                                                                                      "subNodes": [
                                                                                        {
                                                                                          "idNum": 74,
                                                                                          "orderNum": 0,
                                                                                          "pcNode": false,
                                                                                          "linkTo": 0,
                                                                                          "ShowOnlyOnce": false,
                                                                                          "NodeIsActive": true,
                                                                                          "NodePortraitBitmap": "",
                                                                                          "NodeNpcName": "",
                                                                                          "NodeSound": "none",
                                                                                          "conversationText": "Vinazl's arm falls to the floor, and disappears in a cloud of dust. \"It looks like it will be up to you to defend the forest. Please - you must find the source of these abominations and end them.\"",
                                                                                          "IsExpanded": true,
                                                                                          "subNodes": [
                                                                                            {
                                                                                              "idNum": 75,
                                                                                              "orderNum": 0,
                                                                                              "pcNode": true,
                                                                                              "linkTo": 0,
                                                                                              "ShowOnlyOnce": false,
                                                                                              "NodeIsActive": true,
                                                                                              "NodePortraitBitmap": "",
                                                                                              "NodeNpcName": "",
                                                                                              "NodeSound": "none",
                                                                                              "conversationText": "\"I just want to get out of the Raventhal. I am no hero.\"",
                                                                                              "IsExpanded": true,
                                                                                              "subNodes": [
                                                                                                {
                                                                                                  "idNum": 76,
                                                                                                  "orderNum": 0,
                                                                                                  "pcNode": false,
                                                                                                  "linkTo": 0,
                                                                                                  "ShowOnlyOnce": false,
                                                                                                  "NodeIsActive": true,
                                                                                                  "NodePortraitBitmap": "",
                                                                                                  "NodeNpcName": "",
                                                                                                  "NodeSound": "none",
                                                                                                  "conversationText": "\"Who is? But even if you're not driven by the desire to do good, if you want to escape the forest you will need to end this corruption.\"",
                                                                                                  "IsExpanded": true,
                                                                                                  "subNodes": [
                                                                                                    {
                                                                                                      "idNum": 77,
                                                                                                      "orderNum": 0,
                                                                                                      "pcNode": true,
                                                                                                      "linkTo": 0,
                                                                                                      "ShowOnlyOnce": false,
                                                                                                      "NodeIsActive": true,
                                                                                                      "NodePortraitBitmap": "",
                                                                                                      "NodeNpcName": "",
                                                                                                      "NodeSound": "none",
                                                                                                      "conversationText": "\"Can you give me any clues? What is it, where do I look?\"",
                                                                                                      "IsExpanded": true,
                                                                                                      "subNodes": [
                                                                                                        {
                                                                                                          "idNum": 78,
                                                                                                          "orderNum": 0,
                                                                                                          "pcNode": false,
                                                                                                          "linkTo": 0,
                                                                                                          "ShowOnlyOnce": false,
                                                                                                          "NodeIsActive": true,
                                                                                                          "NodePortraitBitmap": "",
                                                                                                          "NodeNpcName": "",
                                                                                                          "NodeSound": "none",
                                                                                                          "conversationText": "\"A spirit.....\" Vinazl's eyes are clouding over. \"A dark spirit. An ancient. I don't know.... where.\"",
                                                                                                          "IsExpanded": true,
                                                                                                          "subNodes": [
                                                                                                            {
                                                                                                              "idNum": 79,
                                                                                                              "orderNum": 0,
                                                                                                              "pcNode": true,
                                                                                                              "linkTo": 0,
                                                                                                              "ShowOnlyOnce": false,
                                                                                                              "NodeIsActive": true,
                                                                                                              "NodePortraitBitmap": "",
                                                                                                              "NodeNpcName": "",
                                                                                                              "NodeSound": "none",
                                                                                                              "conversationText": "Continue.",
                                                                                                              "IsExpanded": true,
                                                                                                              "subNodes": [
                                                                                                                {
                                                                                                                  "idNum": 80,
                                                                                                                  "orderNum": 0,
                                                                                                                  "pcNode": false,
