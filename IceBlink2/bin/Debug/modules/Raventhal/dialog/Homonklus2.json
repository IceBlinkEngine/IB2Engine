                                  "NodePortraitBitmap": "ezzbel.png",
                                  "NodeNpcName": "",
                                  "NodeSound": "none",
                                  "conversationText": "\"<Race>, this creature is fooling you. It is a thing of darkness. I will not travel with it.\"",
                                  "IsExpanded": true,
                                  "subNodes": [
                                    {
                                      "idNum": 13,
                                      "orderNum": 0,
                                      "pcNode": true,
                                      "linkTo": 0,
