                              "NodeIsActive": true,
                              "NodePortraitBitmap": "",
                              "NodeNpcName": "",
                              "NodeSound": "none",
                              "conversationText": "\"Move into the light.\"",
                              "IsExpanded": true,
                              "subNodes": [
                                {
                                  "idNum": 8,
                                  "orderNum": 0,
                                  "pcNode": false,
                                  "linkTo": 0,
