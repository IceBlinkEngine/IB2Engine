                                              "ShowOnlyOnce": false,
                                              "NodeIsActive": true,
                                              "NodePortraitBitmap": "",
                                              "NodeNpcName": "",
                                              "NodeSound": "none",
                                              "conversationText": "\"You are not what you seem.\"",
                                              "IsExpanded": true,
                                              "subNodes": [
