              "ShowOnlyOnce": false,
              "NodeIsActive": true,
              "NodePortraitBitmap": "",
              "NodeNpcName": "",
              "NodeSound": "none",
              "conversationText": "\"I'm not the Wizard. I'm a wizard. Another one. A different one.\"",
              "IsExpanded": true,
              "subNodes": [
                {
                  "idNum": 3,
                  "orderNum": 0,
                  "pcNode": false,
