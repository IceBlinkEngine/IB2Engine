              "ShowOnlyOnce": false,
              "NodeIsActive": true,
              "NodePortraitBitmap": "",
              "NodeNpcName": "",
              "NodeSound": "none",
              "conversationText": "\"Who is that? Who is down there?\"",
              "IsExpanded": true,
              "subNodes": [
                {
                  "idNum": 3,
                  "orderNum": 0,
                  "pcNode": false,
                  "linkTo": 0,
                  "ShowOnlyOnce": false,
                  "NodeIsActive": true,
                  "NodePortraitBitmap": "",
                  "NodeNpcName": "",
                  "NodeSound": "none",
                  "conversationText": "\"My name is Vinazl. Now you know. Please leave.\"",
                  "IsExpanded": true,
                  "subNodes": [
                    {
                      "idNum": 4,
                      "orderNum": 0,
                      "pcNode": true,
                      "linkTo": 0,
                      "ShowOnlyOnce": false,
                      "NodeIsActive": true,
                      "NodePortraitBitmap": "",
                      "NodeNpcName": "",
                      "NodeSound": "none",
                      "conversationText": "\"Vinazl, I need to get out of the forest. Can you help?\"",
                      "IsExpanded": true,
                      "subNodes": [
