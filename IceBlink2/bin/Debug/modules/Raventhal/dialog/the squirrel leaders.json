                                                      "NodeIsActive": true,
                                                      "NodePortraitBitmap": "",
                                                      "NodeNpcName": "",
                                                      "NodeSound": "none",
                                                      "conversationText": "\"End how?\"",
                                                      "IsExpanded": true,
