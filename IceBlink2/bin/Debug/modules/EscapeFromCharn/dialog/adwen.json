                      "subNodes": [
                        {
                          "idNum": 16,
                          "orderNum": 0,
                          "pcNode": false,
                          "linkTo": 0,
                          "ShowOnlyOnce": false,
                          "NodeIsActive": true,
                          "NodePortraitBitmap": "",
                          "NodeNpcName": "",
                          "NodeSound": "none",
                          "conversationText": "Yes, Elos has visited me and given us instructions on how to proceed.  We are to leave Charn for good.  But first we must retrieve some items that have been lost and forgotten.",
