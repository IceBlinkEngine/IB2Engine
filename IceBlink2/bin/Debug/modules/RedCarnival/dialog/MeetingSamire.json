          "NodeIsActive": true,
          "NodePortraitBitmap": "",
          "NodeNpcName": "",
          "NodeSound": "none",
          "conversationText": "\"You've got it! So many thanks for diving through the dark and cold for me!\"",
          "IsExpanded": true,
          "subNodes": [
            {
              "idNum": 16,
              "orderNum": 0,
              "pcNode": true,
              "linkTo": 0,
              "ShowOnlyOnce": false,
